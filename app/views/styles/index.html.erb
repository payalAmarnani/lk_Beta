<p id="notice"><%= notice %></p>

<h1>Listing Styles</h1>

<table>
  <thead>
    <tr>
      <th>Orderitem</th>
      <th>Cuff</th>
      <th>Collar</th>
      <th>Fit</th>
      <th>Length type</th>
      <th>Contrastcollarcuff</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @styles.each do |style| %>
      <tr>
        <td><%= style.OrderItem %></td>
        <td><%= style.cuff %></td>
        <td><%= style.collar %></td>
        <td><%= style.fit %></td>
        <td><%= style.length_type %></td>
        <td><%= style.contrastCollarCuff %></td>
        <td><%= link_to 'Show', style %></td>
        <td><%= link_to 'Edit', edit_style_path(style) %></td>
        <td><%= link_to 'Destroy', style, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Style', new_style_path %>
